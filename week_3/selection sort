class Solution
{
	int  select(int arr[], int i)
	{
	   return (arr[i]); 
        // code here such that selectionSort() sorts arr[]
	}
	
	void selectionSort(int arr[], int n)
	{
	    for(int i=0;i<arr.length-1;i++){
	        for(int j=i+1;j<n;j++){
	            if(arr[i]>arr[j]){
	                int temp=arr[i];
	                arr[i]=arr[j];
	                arr[j]=temp;
	            }
	        }
	    }
	    //code here
	}
}
