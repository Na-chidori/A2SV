class Solution {
    public List<Integer> targetIndices(int[] nums, int target) {
      ArrayList<Integer> count_Temp= new ArrayList<>();
        Arrays.sort(nums);
        for(int i=0;i<nums.length;i++){
            if (nums[i]==target){
                count_Temp.add(i);
            }
        }
        return (count_Temp);
        
    }
} 
